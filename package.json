{
  "name": "adb-web-manager",
  "version": "0.1.0",
  "main": "main.cjs",
  "private": true,
  "type": "module",
  "description": "A web manager for ADB",
  "author": "Mint@notero",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:dev": "next build",
    "package": "electron-builder",
    "clean": "rimraf build",
    "package:dev": "npm run clean && npm run build &&  electron-builder",
    "electron": "electron .",
    "electron:dev": "npm run clean && npm run build && electron .",
    "lint": "next lint"
 

  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.7",
    "busboy": "^1.6.0",
    "dotenv": "^16.4.7",
    "formidable": "^3.5.2",
    "lucide-react": "^0.477.0",
    "multer": "^1.4.5-lts.1",
    "next": "^15.1.7",
    "node-nmap": "^4.0.0",
    "ping": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-modal": "^3.16.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "cross-env": "^7.0.3",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1"
  },
  "build": {
    "asar": true,
    "compression": "store",
    "appId": "com.adb.webmanager",
    "productName": "ADB Web Manager",
    "files": [
      "main.cjs",
      "package.json",
      ".next/**/*",
      "public/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "build/exe"
    },
    "win": {
      "target": "nsis"
    }
  }
}
